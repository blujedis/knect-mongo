{
  "name": "knect-mongo",
  "version": "1.1.12",
  "description": "Mongodb connection and model helper.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc",
    "watch": "npm run build -- -w",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json",
    "typedoc": "typedoc ./src --options ./typedoc.json --out ./docs",
    "docs": "npm run typedoc && touch ./docs/.nojekll",
    "test": "mocha test/**/*.spec.ts* --require ./register.js --bail",
    "prep": "npm run clean && npm run lint && npm run test && npm run docs && npm run build"
  },
  "author": "Blujedis <blujedicorp@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/blujedis/knect-mongo.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.25",
    "@types/node": "^11.11.3",
    "@types/yup": "^0.26.12",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "mongodb": "^3.2.3",
    "yup": "^0.27.0"
  }
}
